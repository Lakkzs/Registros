<select type="select" class="form-control" name="{{name}}" id="{{id}}">
    {{#if datos.CIUDAD_COLABORADOR}}
        <option value="{{datos.CIUDAD_COLABORADOR}}" selected hidden> {{datos.CIUDAD_COLABORADOR}} </option>
    {{else if datos}}
        <option value="{{datos}}" selected hidden> {{datos}} </option>
    {{else}}
        <option value="Nada" selected hidden> Seleccione </option>
    {{/if}}
    {{#each municipios}}
    <option value="{{Descripcion}}"> {{Descripcion}} </option>
    {{/each}}
</select>